import java.util.Scanner;
class Bank 
{
	public static void main(String[] args) 
	{
		System.out.println("Enter an amount: ");
		Scanner s1=new Scanner(System.in);
		int amt=s1.nextInt();
		if(amt>=2000){
			System.out.println("2000 X " +  (amt/2000));
			amt=amt% 2000;
		}
		if(amt>=1000){
			System.out.println("2000 X " +(amt/1000));
			amt=amt% 1000;
		}
		if(amt>=500){
			System.out.println("2000 X " +(amt/500));
			amt=amt% 500;
		}
		if(amt>=200){
			System.out.println("2000 X " +(amt/200));
			amt=amt%200;
		}
		if(amt>=100){
			System.out.println("2000 X " +(amt/100));
			
		}

	}
}
